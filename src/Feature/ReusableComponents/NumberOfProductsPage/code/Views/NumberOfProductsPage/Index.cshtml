@using Sitecore.Data.Items
@model Websites.Feature.ReusableComponents.NumberOfProductsPage.Models.PagesListModel

<h5 class="m-text14 p-b-5">
    @Model.Title
</h5>
<div class="flex-sb-m flex-w p-b-35">
    <div class="flex-w">
        <div class="rs2-select2 bo4 of-hidden w-size12 m-t-5 m-b-5 m-r-10">
            <select class="selection-2" name="size">
                @foreach (Item item in Model.PagesList)
                {
                    if (item != null)
                    {

                        <option> @Html.Sitecore().Field("Value", item) </option>
                    }
                }
            </select>
        </div>
    </div>
</div>


